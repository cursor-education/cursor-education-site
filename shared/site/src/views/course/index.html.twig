    {% include 'course/_header.html.twig' %}

    {% include 'course/_description.html.twig' %}

    {% if course.technologies %}
      {% include 'course/_technologies.html.twig' %}
    {% endif %}

    {% if coursePlan %}
      {% include 'course/_program.html.twig' %}
    {% endif %}

    {% include 'course/_form.html.twig' %}
    {% include 'course/_footer.html.twig' %}
{% endblock %}